{
  "Routes": [
    {
      "SwaggerKey": "identity",
      "DownstreamPathTemplate" : "/api/{everything}",
      "DownstreamScheme" : "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identityservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate" : "/api/identity/{everything}",
      "UpstreamHttpMethod" : [ "Get", "Post" ]
    },
    {
      "SwaggerKey": "profile",
      "DownstreamPathTemplate" : "/api/{everything}",
      "DownstreamScheme" : "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "profileservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate" : "/api/profile/{everything}",
      "UpstreamHttpMethod" : [ "Get", "Post", "Put", "Delete" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:80"
  },
  "SwaggerEndPoints": [
    {
      "Key": "identity",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": "http://identityservice/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "profile",
      "Config": [
        {
          "Name": "Profile API",
          "Version": "v1",
          "Url": "http://profileservice/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
